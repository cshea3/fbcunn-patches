+#  exit 1
+#fi
+#unzip gtest-1.7.0.zip
+tar -xvf googletest-release-1.8.0.tar.gz
+mv googletest-release-1.8.0 gtest-1.7.0
 # Build in a separate directory
 mkdir -p build
 cd build
